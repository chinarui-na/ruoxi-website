<svg xmlns="http://www.w3.org/2000/svg" width="800" height="600" viewBox="0 0 800 600">
  <defs>
    <linearGradient id="grad1" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#667eea;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#764ba2;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="800" height="600" fill="url(#grad1)"/>
  
  <!-- Team illustration -->
  <g opacity="0.9">
    <!-- Person 1 -->
    <circle cx="250" cy="250" r="40" fill="#fff" opacity="0.3"/>
    <circle cx="250" cy="230" r="25" fill="#fff" opacity="0.5"/>
    <rect x="230" y="260" width="40" height="60" rx="20" fill="#fff" opacity="0.5"/>
    
    <!-- Person 2 -->
    <circle cx="400" cy="250" r="40" fill="#fff" opacity="0.3"/>
    <circle cx="400" cy="230" r="25" fill="#fff" opacity="0.5"/>
    <rect x="380" y="260" width="40" height="60" rx="20" fill="#fff" opacity="0.5"/>
    
    <!-- Person 3 -->
    <circle cx="550" cy="250" r="40" fill="#fff" opacity="0.3"/>
    <circle cx="550" cy="230" r="25" fill="#fff" opacity="0.5"/>
    <rect x="530" y="260" width="40" height="60" rx="20" fill="#fff" opacity="0.5"/>
  </g>
  
  <!-- Connecting lines -->
  <line x1="250" y1="290" x2="400" y2="290" stroke="#fff" stroke-width="3" opacity="0.4"/>
  <line x1="400" y1="290" x2="550" y2="290" stroke="#fff" stroke-width="3" opacity="0.4"/>
  
  <!-- Icons representing collaboration -->
  <g transform="translate(350, 350)">
    <circle cx="0" cy="0" r="50" fill="#fff" opacity="0.2"/>
    <path d="M -20,-10 L -20,10 L 0,20 L 20,10 L 20,-10 L 0,-20 Z" fill="#fff" opacity="0.6"/>
  </g>
  
  <!-- Text -->
  <text x="400" y="480" font-family="Arial, sans-serif" font-size="36" fill="#fff" text-anchor="middle" font-weight="bold">团队协作</text>
  <text x="400" y="520" font-family="Arial, sans-serif" font-size="20" fill="#fff" text-anchor="middle" opacity="0.8">专业团队 · 卓越服务</text>
</svg>
